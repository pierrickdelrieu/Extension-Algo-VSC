{	
	"Fonction": {
		"prefix": "Fonction",
		"body": [
			"Fonction : ${1:nom}(${2:parametres}) : ${3:typeretour}",
			"Variables locales : ${4:var} : ${5:type}",
			"Données : ${6:param} : ${7:type}",
			"Début",
			"\t",
			"Fin\n"
		],
		"description": "Insertion d'un bloc de fonction"
	},




	"Debut": {
		"prefix": "Début",
		"body": [
			"Début",
			"\t$0",
			"Fin"
		],
		"description": "Insertion d'un bloc Début Fin"
	},




	"Structure": {
		"prefix": "Structure",
		"body": [
			"Structure: ${1:nom}",
			"\t${2:var} : ${3:type}",
			"Fin Structure"
		],
		"description": "Insertion d'un bloc de structure"
	},




	"Pour": {
		"prefix": "Pour",
		"body": [
			"Pour (${1:i} allant de ${2:a} à ${3:b})",
			"\t",
			"Fin Pour"
		],
		"description": "Insertion d'un bloc de boucle Pour"
	},




	"Tant que": {
		"prefix": "Tant que",
		"body": [
			"Tant que (${1:condition})",
			"\t",
			"Fin Tant que"
		],
		"description": "Insertion d'un bloc Tant que"
	},




	"Faire tant que": {
		"prefix": "Faire",
		"body": [
			"Faire",
			"\t",
			"Tant que(${1:conditon})"
		],
		"description": "Insertion d'un bloc Faire Tant que"
	},
	



	"Si": {
		"prefix": "Si",
		"body": [
			"Si (${1:conditon})",
			"\t",
			"Fin Si"
		],
		"description": "Insertion d'un bloc Si"
	},
	

	
	"Sinon si": {
		"prefix": "Sinon si",
		"body": [
			"Sinon Si (${1:conditon})",
			"\t",
			"Fin Si"
		],
		"description": "Insertion d'un bloc Sinon Si"
	},


	"Sinon": {
		"prefix": "Sinon",
		"body": [
			"Sinon",
			"\t$0",
			"Fin Si"
		],
		"description": "Insertion d'un bloc Sinon"
	},


	"saisir": {
		"prefix":"saisir",
		"body": ["saisir(${1:type})"],
		"description": "Fonction saisir renvoie la valeur saisie par l'utilisateur"
	},


	"retourner": {
		"prefix":"retourner",
		"body": ["retourner(${1:var})"],
		"description": "Valeur de retour de la fonction"
	},


	"taille": {
		"prefix":"taille",
		"body": ["taille(${1:tab})"],
		"description": "Fonction taille renvoie la taille d'un tableau 1D"
	},


	"reserver": {
		"prefix":"reserver",
		"body": ["reserver(${1:type})"],
		"description": "Fonction reserver, permet l'allocation dynamique d'une variable"
	},


	"liberer": {
		"prefix":"liberer",
		"body": ["liberer(${1:var})"],
		"description": "Fonction liberer, permet la désallocation dynamique d'une variable"
	}
}